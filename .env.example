# Frontend / Node.js environment variables (non-secret)
# Copy to .env and adjust as needed for local development.

# Vite/Frontend config
# Node backend (auth, practices, memberships, forms)
# Dev: https://staging-api.blawby.com
# Prod: https://production-api.blawby.com
VITE_AUTH_SERVER_URL=https://staging-api.blawby.com
VITE_REMOTE_API_URL=https://staging-api.blawby.com

# Worker base (conversations/chat endpoints)
# Dev: http://localhost:8787
# Prod: https://ai.blawby.com
VITE_API_URL=http://localhost:8787

# Frontend base for checkout callbacks
# Dev: http://localhost:5137
# Prod: https://ai.blawby.com
VITE_APP_BASE_URL=http://localhost:5137

VITE_STRIPE_PUBLISHABLE_KEY=pk_test_example
VITE_ONESIGNAL_APP_ID=your-onesignal-app-id-here

# Development practice seeding (used by scripts/dev-seed-practice.ts)
DEV_SEED_BASE_URL=http://localhost:8787
DEV_SEED_AUTH_URL=http://localhost:8787
DEV_SEED_USER_EMAIL=dev@example.com
DEV_SEED_USER_PASSWORD=dev-password
DEV_SEED_BUSINESS_EMAIL=test+practice@example.com
DEV_SEED_BUSINESS_PHONE=+17025550123
DEV_SEED_CONSULTATION_FEE=150
DEV_SEED_PRACTICE_NAME="Dev Practice"
DEV_SEED_PRACTICE_SLUG=dev-practice
VITE_DEFAULT_PRACTICE_PHONE=+17025550123
VITE_DEFAULT_CONSULTATION_FEE=150

# E2E test credentials (Playwright)
E2E_PRACTICE_ID=your-practice-uuid
E2E_PRACTICE_SLUG=your-practice-slug
E2E_OWNER_EMAIL=owner@example.com
E2E_OWNER_PASSWORD=change-me
E2E_CLIENT_EMAIL=client@example.com
E2E_CLIENT_PASSWORD=change-me
E2E_REMOTE_API_URL=https://staging-api.blawby.com
E2E_WORKER_URL=http://localhost:8787
E2E_BASE_URL=https://local.blawby.com
E2E_PAYMENT_MODE=auto
