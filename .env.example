# Frontend environment variables for Vite (Cloudflare Pages build + local dev)
VITE_BACKEND_API_URL=https://staging-api.blawby.com

# Worker base for conversations/chat (dev local; prod: https://ai.blawby.com)
VITE_WORKER_API_URL=http://localhost:8787

# Frontend base URL for checkout callbacks (dev local; prod: https://ai.blawby.com)
VITE_APP_BASE_URL=http://localhost:5173

VITE_ONESIGNAL_APP_ID=your-onesignal-app-id-here

# Stripe publishable key for intake payments
VITE_STRIPE_KEY=pk_test_example

# Tunnel token for local dev (scripts/run-tunnel.ts)
CLOUDFLARE_TUNNEL_TOKEN=your-cloudflare-tunnel-token

# E2E test credentials (Playwright)
E2E_PRACTICE_ID=your-practice-uuid
E2E_PRACTICE_SLUG=your-practice-slug
E2E_OWNER_EMAIL=owner@example.com
E2E_OWNER_PASSWORD=change-me
E2E_CLIENT_EMAIL=client@example.com
E2E_CLIENT_PASSWORD=change-me
E2E_REMOTE_API_URL=https://staging-api.blawby.com
E2E_WORKER_URL=http://localhost:8787
E2E_BASE_URL=https://local.blawby.com
E2E_PAYMENT_MODE=auto
